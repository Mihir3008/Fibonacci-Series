#include <iostream>  
using namespace std;  
int main() 
{  
  int a1=0,a2=1,a3,i,num;    
 cout<<"Enter the number of elements: ";    
 cin>>num;    
 cout<<a1<<" "<<a2<<" "; //printing 0 and 1    
 for(i=2;i<num;++i) //loop starts from 2 because 0 and 1 are already printed    
 {    
  a3=a1+a2;    
  cout<<a3<<" ";    
  a1=a2;    
  a2=a3;    
 }    
return 0;  
}  
